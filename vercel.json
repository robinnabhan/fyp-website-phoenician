{
    "version": 2,
    "builds": [
      {
        "src": "/my-fyp/api/server.py",
        "use": "@vercel/static-build",
        "config": { "distDir": "my-fyp/build" }
      },
      {
        "src": "/my-fyp/api/server.py",
        "use": "@vercel/python"
    
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "my-fyp/api/server.py" },
      { "src": "/(.*)", "dest": "/my-fyp/$1" }
    ]
  }
  